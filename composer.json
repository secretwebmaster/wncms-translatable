{
    "name": "secretwebmaster/wncms-translatable",
    "description": "Make any laravel model translatable",
    "keywords": [
        "secretwebmaster",
        "wncms",
        "laravel",
        "translate",
        "translatable"
    ],
    "homepage": "https://github.com/secretwebmaster/wncms-translatable",
    "license": "MIT",
    "authors": [
        {
            "name": "文尼先生",
            "email": "earnbyshare2016@gmail.com",
            "homepage": "https://wntheme.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "secretwebmaster/wncms-translatable": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Wncms\\Translatable\\": "src/"
        }
    },
    "extra":{
        "laravel": [
          "src/Wncms/TranslatableServiceProvider"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}